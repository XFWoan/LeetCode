    "version": "0.2.0",
    "configurations": [

        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/SolutionDebug/bin/Debug/netcoreapp3.1/SolutionDebug.dll",
            "args": [],
            "cwd": "${workspaceFolder}/SolutionDebug",
            "console": "internalConsole",
            "stopAtEntry": true
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}